                    "bottom": 40,
                    "dimensions": {
                    "side": 65,
                    "sidePreview": 65,
                    "frame": 3,
                    "margin": 5,
                    "label": false,
                    "vertical": true
                   }
                }
            },
            {
                "name": "Identify",
                "showIn": ["Settings"],
                "cfg": {
                    "style": {
                        "position": "absolute",
                        "width": "100%",
                        "bottom": "0px",
                        "zIndex": 1023,
                        "maxHeight": "70%",
                        "marginBottom": 0
                    },
                    "draggable": false,
                    "collapsible": true,
                    "viewerOptions": {
                        "container": "{context.ReactSwipe}",
                        "header": "{context.SwipeHeader}",
                        "collapsible": false
                    },
                    "bodyClass": "mobile-feature-info"
